DEFINE DATA LOCAL
1 AUTO   VIEW AUTOMOBILES
  2 MODEL-YEAR-MANU     2 REDEFINE MODEL-YEAR-MANU
    3 #YEAR (N4)        (HD='Year')
    3 #MANU (A32)       (HD='MANU')
END-DEFINE
LIMIT 9
HISTOGRAM AUTOFOR MODEL-YEAR-MANU
  DISPLAY
          #YEAR
          #MANU
          *NUMBER
END-HISTOGRAM
END